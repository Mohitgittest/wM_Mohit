<?xml version="1.0"?>
<scheduleTasks>
    <tasks>
        <type>repeat</type>
        <service>amsUMPing:checkLog</service>
            <Repeating>
                 <runAsUser>AMSWMSched</runAsUser>
                  <persistJob>true</persistJob>
                  <runInCluster>false</runInCluster>
                  <inputs/>
                  <interval>300</interval>
                  <doNotOverlap>true</doNotOverlap>
            </Repeating>
    </tasks>
    <tasks>
        <type>repeat</type>
        <service>amsUMPing:checkUM</service>
            <Repeating>
                 <runAsUser>AMSWMSched</runAsUser>
                  <persistJob>true</persistJob>
                  <runInCluster>false</runInCluster>
                  <inputs/>
                  <interval>300</interval>
                  <doNotOverlap>true</doNotOverlap>
            </Repeating>
    </tasks>
    <tasks>
        <type>repeat</type>
        <service>amsUMPing:pingCOCN</service>
            <Repeating>
                 <runAsUser>AMSWMSched</runAsUser>
                  <persistJob>true</persistJob>
                  <runInCluster>false</runInCluster>
                  <inputs/>
                  <interval>120</interval>
                  <doNotOverlap>true</doNotOverlap>
            </Repeating>
    </tasks>
    <tasks>
        <type>repeat</type>
        <service>amsUMPing:logSLibCOCN</service>
        <target>$all</target>
        <Repeating>
          <runAsUser>AMSWMSched</runAsUser>
          <persistJob>true</persistJob>
          <inputs/>
          <interval>60</interval>
          <doNotOverlap>true</doNotOverlap>
        </Repeating>
    </tasks>
</scheduleTasks>
